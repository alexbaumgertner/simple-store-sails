block('page')
    .mod('section', 'admin')
    .content()(function() {
    var data = this.ctx.data;

    return [
        {
            block: 'menu',
            mods: {theme: 'islands', size: 'm', section: 'admin'}
        },
        {
            block: 'controller',
            elem: data.req.options.controller,
            mods: {action: data.req.options.action},
            data: data
        },
        applyNext()];

});
